<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="approveRegistrationRoot" prefHeight="288.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.mosip.registration.controller.eodapproval.RegistrationApprovalController">
   <children>
      <AnchorPane fx:id="approveRegistrationRootSubPane" prefHeight="200.0" prefWidth="200.0">
         <children>
            <AnchorPane fx:id="imageAnchorPane" layoutX="268.0" layoutY="10.0" prefHeight="278.0" prefWidth="370.0">
               <children>
                  <ImageView fx:id="imageView" fitHeight="272.0" fitWidth="366.0" layoutX="2.0" layoutY="4.0" onMouseClicked="#openAckForm" pickOnBounds="true" preserveRatio="true" />
               </children>
            </AnchorPane>
            <TableView fx:id="table" layoutX="14.0" layoutY="10.0" prefHeight="278.0" prefWidth="247.0">
               <columns>
                  <TableColumn fx:id="id" prefWidth="245.0" text="Registration ID" />
                  <TableColumn fx:id="acknowledgementFormPath" prefWidth="199.0" text="Registrtion ID" visible="false" />
               </columns>
            </TableView>
            <Button fx:id="submitBtn" layoutX="819.0" layoutY="263.0" mnemonicParsing="false" onAction="#submit" prefHeight="10.0" prefWidth="80.0" text="Authenticate">
               <font>
                  <Font size="10.0" />
               </font>
            </Button>
            <ToggleButton fx:id="approvalBtn" layoutX="644.0" layoutY="16.0" mnemonicParsing="false" onAction="#approvePacket" prefHeight="20.0" prefWidth="80.0" text="Approve">
               <font>
                  <Font size="10.0" />
               </font>
            </ToggleButton>
            <ToggleButton fx:id="rejectionBtn" layoutX="732.0" layoutY="16.0" mnemonicParsing="false" onAction="#rejectPacket" prefHeight="20.0" prefWidth="80.0" text="Reject">
               <font>
                  <Font size="10.0" />
               </font>
            </ToggleButton>
            <ToggleButton fx:id="onHoldBtn" layoutX="819.0" layoutY="14.0" mnemonicParsing="false" onAction="#onHoldPacket" prefHeight="20.0" prefWidth="80.0" text="On Hold">
               <font>
                  <Font size="10.0" />
               </font>
            </ToggleButton>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
